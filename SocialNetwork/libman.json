{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [

    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@6.0.6",
      "destination": "wwwroot/lib/microsoft/signalr/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr-protocol-msgpack@6.0.6",
